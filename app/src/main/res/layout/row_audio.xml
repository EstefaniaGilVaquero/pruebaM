<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cvRow"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="5dp"
    android:layout_marginBottom="20dp"
    android:background="?attr/selectableItemBackgroundBorderless"
    app:cardBackgroundColor="@color/colorPrimary"
    app:cardElevation="3dp">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <TextView
            android:id="@+id/tvAudioDate"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Date"
            android:textAlignment="center"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toStartOf="@+id/tvCrisis"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tvCrisis"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Crisis"
            android:textAlignment="center"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/tvAudioDate"
            app:layout_constraintTop_toTopOf="parent" />

        <TableRow
            android:id="@+id/tableRow2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="32dp"
            android:layout_marginStart="2dp"
            android:layout_marginTop="8dp"
            android:background="@color/colorBlueDark"
            android:padding="5dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/tableRow"
            app:layout_constraintTop_toBottomOf="@+id/tvCrisis">

            <TextView
                android:id="@+id/tvLeftTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/oidoIzquierdo"
                android:textSize="15sp"
                android:textStyle="bold" />


        </TableRow>

        <TableRow
            android:id="@+id/tableRow3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="32dp"
            android:background="@color/colorBlue"
            android:paddingLeft="5dp"
            android:paddingTop="5dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/tableRow2"
            app:layout_constraintTop_toBottomOf="@+id/tableRow2">

            <TextView
                android:id="@+id/tvFrecuencia05_bis"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/frecuencia05"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="195dp"
                tools:layout_editor_absoluteY="53dp" />

            <TextView
                android:id="@+id/tvLeft05"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="5dp"
                android:text="valor"
                android:textAlignment="textEnd"
                tools:layout_editor_absoluteX="254dp"
                tools:layout_editor_absoluteY="53dp" />


        </TableRow>

        <TableRow
            android:id="@+id/tableRow4"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="32dp"
            android:background="@color/colorBlue"
            android:paddingLeft="5dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/tableRow3"
            app:layout_constraintTop_toBottomOf="@+id/tableRow3">

            <TextView
                android:id="@+id/tvFrecuencia1_bis"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/frecuencia1"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="195dp"
                tools:layout_editor_absoluteY="71dp" />

            <TextView
                android:id="@+id/tvLeft1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="5dp"
                android:text="valor"
                tools:layout_editor_absoluteX="254dp"
                tools:layout_editor_absoluteY="71dp" />

        </TableRow>

        <TableRow
            android:id="@+id/tableRow5"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="32dp"
            android:background="@color/colorBlue"
            android:paddingLeft="5dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/tableRow4"
            app:layout_constraintTop_toBottomOf="@+id/tableRow4">

            <TextView
                android:id="@+id/tvFrecuencia2_bis"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/frecuencia2"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="195dp"
                tools:layout_editor_absoluteY="101dp" />

            <TextView
                android:id="@+id/tvLeft2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="5dp"
                android:text="valor"
                tools:layout_editor_absoluteX="254dp"
                tools:layout_editor_absoluteY="100dp" />

        </TableRow>

        <TableRow
            android:id="@+id/tableRow9"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:layout_marginEnd="32dp"
            android:background="@color/colorBlue"
            android:paddingBottom="5dp"
            android:paddingLeft="5dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/tableRow5"
            app:layout_constraintTop_toBottomOf="@+id/tableRow5">

            <TextView
                android:id="@+id/tvFrecuencia4_bis"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/frecuencia4"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="195dp"
                tools:layout_editor_absoluteY="121dp" />

            <TextView
                android:id="@+id/tvLeft4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="5dp"
                android:text="valor"
                tools:layout_editor_absoluteX="254dp"
                tools:layout_editor_absoluteY="121dp" />
        </TableRow>


        <TableRow
            android:id="@+id/tableRow"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="2dp"
            android:layout_marginStart="32dp"
            android:layout_marginTop="8dp"
            android:background="@color/colorRedDark"
            android:padding="5dp"
            app:layout_constraintEnd_toStartOf="@+id/tableRow2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tvAudioDate">

            <TextView
                android:id="@+id/tvRigthTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/oidoDerecho"
                android:textAlignment="center"
                android:textSize="15sp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="62dp"
                tools:layout_editor_absoluteY="35dp" />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow6"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="4dp"
            android:background="@color/colorRed"
            android:paddingLeft="5dp"
            android:paddingTop="5dp"
            app:layout_constraintEnd_toStartOf="@+id/tableRow3"
            app:layout_constraintStart_toStartOf="@+id/tableRow"
            app:layout_constraintTop_toBottomOf="@+id/tableRow">

            <TextView
                android:id="@+id/tvFrecuencia05"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/frecuencia05"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="62dp"
                tools:layout_editor_absoluteY="53dp" />

            <TextView
                android:id="@+id/tvRigth05"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="5dp"
                android:text="valor"
                tools:layout_editor_absoluteX="115dp"
                tools:layout_editor_absoluteY="53dp" />

        </TableRow>

        <TableRow
            android:id="@+id/tableRow7"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="4dp"
            android:background="@color/colorRed"
            android:paddingLeft="5dp"
            app:layout_constraintEnd_toStartOf="@+id/tableRow4"
            app:layout_constraintStart_toStartOf="@+id/tableRow6"
            app:layout_constraintTop_toBottomOf="@+id/tableRow6">

            <TextView
                android:id="@+id/tvFrecuencia1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/frecuencia1"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="62dp"
                tools:layout_editor_absoluteY="71dp" />

            <TextView
                android:id="@+id/tvRigth1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="5dp"
                android:text="valor"
                tools:layout_editor_absoluteX="115dp"
                tools:layout_editor_absoluteY="71dp" />

        </TableRow>


        <TableRow
            android:id="@+id/tableRow8"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="4dp"
            android:background="@color/colorRed"
            android:paddingLeft="5dp"
            app:layout_constraintEnd_toStartOf="@+id/tableRow5"
            app:layout_constraintStart_toStartOf="@+id/tableRow7"
            app:layout_constraintTop_toBottomOf="@+id/tableRow7">

            <TextView
                android:id="@+id/tvFrecuencia2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/frecuencia2"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="62dp"
                tools:layout_editor_absoluteY="91dp" />

            <TextView
                android:id="@+id/tvRigth2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="5dp"
                android:text="valor"
                tools:layout_editor_absoluteX="115dp"
                tools:layout_editor_absoluteY="91dp" />
        </TableRow>

        <TableRow
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:layout_marginEnd="4dp"
            android:background="@color/colorRed"
            android:paddingBottom="5dp"
            android:paddingLeft="5dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/tableRow9"
            app:layout_constraintStart_toStartOf="@+id/tableRow8"
            app:layout_constraintTop_toBottomOf="@+id/tableRow8">

            <TextView
                android:id="@+id/tvFrecuencia4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/frecuencia4"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="62dp"
                tools:layout_editor_absoluteY="119dp" />

            <TextView
                android:id="@+id/tvRigth4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="5dp"
                android:text="valor"
                tools:layout_editor_absoluteX="115dp"
                tools:layout_editor_absoluteY="119dp" />

        </TableRow>


    </android.support.constraint.ConstraintLayout>


</android.support.v7.widget.CardView>